{
  "name": "@qiwi/tech-radar",
  "version": "1.0.1",
  "main": "./src/main/js/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "test": "npm-run-all -p -l lint test:unit",
    "test:unit": "yarn jest",
    "lint": "eslint src/{main,test}/**/*.js",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "release": "npx -p @qiwi/semrel-toolkit semrel",
    "generate": "node ./src/main/js/cli.mjs --input \"data/**/*.{csv,json,yml}\"  --output dist --base-prefix tech-radar --autoscope true && touch dist/.nojekyll"
  },
  "prettier": "prettier-config-qiwi",
  "exports": {
    "import": "./src/main/js/index.js"
  },
  "bin": {
    "techradar": "./src/main/js/cli.mjs"
  },
  "devDependencies": {
    "@qiwi/npm-run-all": "^4.1.7",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-qiwi": "^1.13.2",
    "jest": "^27.0.6",
    "jest-esm-transformer": "^1.0.0",
    "jsdoc": "^3.6.7",
    "prettier": "^2.3.2",
    "prettier-config-qiwi": "^1.4.2",
    "terser": "^5.7.1",
    "typescript": "*"
  },
  "engines": {
    "node": ">=12.12",
    "npm": ">=5.6"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "ajv": "^8.6.1",
    "csv-parse": "^4.16.0",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.4",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "meow": "^10.0.1"
  }
}
